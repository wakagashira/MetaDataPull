# Copy this file to .env and update with your credentials

SQL_DRIVER=ODBC Driver 18 for SQL Server
SQL_SERVER=localhost
SQL_DATABASE=SalesforceMeta
SQL_USERNAME=your_user
SQL_PASSWORD=your_password
SALESFORCE_ORG=Prod
SF_CLI=sf.cmd

# Control sync sections
SYNC_FIELDS=true
SYNC_USAGE=true

# Keep flow XML files after parsing (true/false)
KEEP_FLOW_XML=false

# Debug flow parsing (prints every tag and field reference found)
DEBUG_FLOW_PARSE=false
